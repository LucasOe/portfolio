{
	"printWidth": 120,
	"useTabs": true,
	"tabWidth": 4,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "es5",
	"singleAttributePerLine": false,
	"endOfLine": "lf",
	"plugins": ["@prettier/plugin-oxc", "@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
	"importOrder": ["<BUILTIN_MODULES>", "", "<THIRD_PARTY_MODULES>", "", "^(@/.*)$", "^[.]"]
}
